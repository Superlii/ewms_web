@font-face {
  font-family: 'webfont';
  font-display: swap;
  src: url('//at.alicdn.com/t/webfont_jtt1me6j5zj.eot');
  /* IE9*/
  src: url('//at.alicdn.com/t/webfont_jtt1me6j5zj.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */ url('//at.alicdn.com/t/webfont_jtt1me6j5zj.woff2') format('woff2'), url('//at.alicdn.com/t/webfont_jtt1me6j5zj.woff') format('woff'), /* chrome、firefox */ url('//at.alicdn.com/t/webfont_jtt1me6j5zj.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/ url('//at.alicdn.com/t/webfont_jtt1me6j5zj.svg#杨任东竹石体-Bold') format('svg');
  /* iOS 4.1- */
}
* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  list-style: none;
  font-family: 'Microsoft YaHei', '微软雅黑', 'Helvetica Neue', Helvetica, 'PingFang SC', 'Hiragino Sans GB', Arial, sans-serif !important;
}
html,
body {
  width: 100%;
  height: 100%;
  font-size: 62.5%;
  font-family: Arial, 'Hiragino Sans GB', 'Microsoft YaHei', '微软雅黑', 'Helvetica Neue', Helvetica, 'PingFang SC', sans-serif !important;
  /* 默认16px 1rem = 10px */
}
.el-button {
  font-family: '微软雅黑', Arial, 'Microsoft YaHei', 'Helvetica Neue', Helvetica, 'PingFang SC', 'Hiragino Sans GB', sans-serif;
}
.ant-btn {
  font-family: '微软雅黑', Arial, 'Microsoft YaHei', 'Helvetica Neue', Helvetica, 'PingFang SC', 'Hiragino Sans GB', sans-serif;
  font-size: 12px !important;
}
.ant-btn > .anticon + span,
.ant-btn > span + .anticon {
  margin-left: 4px;
}
a {
  cursor: pointer;
  text-decoration: none;
}
.vue-treeselect__control {
  height: 24px !important;
}
::-webkit-scrollbar {
  width: 5px;
  height: 5px;
}
::-webkit-scrollbar-thumb {
  border-radius: 0.2rem;
  height: 5px;
  background-color: #6f7ad3;
}
::-webkit-scrollbar-track {
  box-shadow: inset 0 0 0.5rem rgba(196, 193, 193, 0.2);
  background: #ededed;
  border-radius: 0.5rem;
}
/* fade */
.fade-enter-active,
.fade-leave-active {
  transition: opacity 0.28s;
}
.fade-enter,
.fade-leave-active {
  opacity: 0;
}
/* fade-transform */
.fade-transform-leave-active,
.fade-transform-enter-active {
  opacity: 0;
  transition: all 0.2s;
}
.fade-transform-enter-to {
  opacity: 1;
  transform: translateX(0);
}
.fade-transform-leave-to {
  opacity: 0;
  transform: translateX(60px);
}
/* breadcrumb transition */
.breadcrumb-enter-active,
.breadcrumb-leave-active {
  opacity: 0;
  transition: all 0.2s;
}
.breadcrumb-enter,
.breadcrumb-leave-active {
  opacity: 0;
  transform: translateX(30rem);
}
.breadcrumb-move {
  transition: all 0.5s;
}
.breadcrumb-leave-active {
  position: absolute;
}
.el-select {
  width: 100%;
}
.el-popper.is-customized {
  /* Set padding to ensure the height is 32px */
  padding: 6px 12px;
  background: linear-gradient(to right, #acb6e5, #86fde8);
}
.el-popper.is-customized .el-popper__arrow::before {
  background: linear-gradient(to right, #acb6e5, #86fde8);
  right: 0;
}
.el-form-item__label {
  font-size: 12px !important;
  color: #333;
}
.el-overlay-dialog {
  background: rgba(255, 255, 255, 0.1);
}
.el-dialog {
  border-radius: 10px;
  border: 1px solid #ccc;
  background: rgba(192, 197, 198, 0.2);
  -webkit-backdrop-filter: blur(8px);
  backdrop-filter: blur(8px);
  border-radius: 15px;
  box-shadow: inset 0 0 6px rgba(255, 255, 255, 0.2);
  box-shadow: -5px -5px 5px rgba(0, 0, 0, 0.1), 5px 10px 5px rgba(0, 0, 0, 0.1);
}
.el-dialog .el-dialog__header {
  border-bottom: 1px solid #ccc;
  margin: 0;
}
.el-dialog .el-dialog__title {
  color: #333;
}
.el-dialog .el-dialog__title:before {
  display: inline-block;
  width: 10px;
  height: 10px;
  border-radius: 50%;
  background: #909399;
  content: '';
  margin-right: 8px;
}
.el-dialog .el-dialog__body {
  padding: 10px 20px 0 20px;
}
.el-dialog .el-dialog__footer {
  border-top: 1px solid #ccc;
}
.el-dialog .el-dialog__footer .el-button {
  width: 100px;
  margin: 10px;
  margin-bottom: 0;
  height: 37px;
  background: #fff;
  color: rgba(13, 13, 13, 0.5);
  text-shadow: 1px 1px 1px rgba(255, 255, 255, 0.1);
  border: 1px solid #cccc;
  border-radius: 30px;
  box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);
  transition: all 0.15s ease;
}
.el-dialog .el-dialog__footer .el-button:hover {
  transform: scale(1.01);
  transition: all 0.15s ease;
  color: #6f7ad3;
}
@-webkit-keyframes rotates_r {
  0% {
    -webkit-transform: rotate(0deg);
  }
  50% {
    -webkit-transform: rotate(180deg);
  }
  100% {
    -webkit-transform: rotate(360deg);
  }
}
/*自定义动画类----逆时针旋转（使用这个动画的时候才设置动画执行时间）*/
@-webkit-keyframes rotates_l {
  0% {
    -webkit-transform: rotate(0deg);
  }
  33% {
    -webkit-transform: rotate(-120deg);
  }
  66% {
    -webkit-transform: rotate(-240deg);
  }
  100% {
    -webkit-transform: rotate(-360deg);
  }
}
.rotae_l20 {
  -webkit-animation: rotates_l 20s linear infinite;
}
.rotae_l4 {
  -webkit-animation: rotates_l 4s linear infinite;
}
.rotae_l3 {
  -webkit-animation: rotates_l 3s linear infinite;
}
